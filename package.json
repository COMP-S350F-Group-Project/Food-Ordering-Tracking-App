{
  "name": "foot_odering_tracking_app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:backend",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "setup": "npm ci --ignore-scripts && npm --prefix frontend ci",
    "build:frontend": "npm --prefix frontend run build",
    "build:all": "npm run build && npm run build:frontend",
    "start:all": "npm run build:all && node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/socket.io": "^3.0.2",
    "@types/morgan": "^1.9.9",
    "concurrently": "^9.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
